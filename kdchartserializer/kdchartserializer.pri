include (${TOP_SOURCE_DIR}/variables.pri)

QT += svg
QT += xml

CONFIG( static ) {
  DEFINES += KDCHARTSERIALIZER_STATICLIB
} else {
  DEFINES += KDCHARTSERIALIZER_SHAREDLIB
  win32 {
    DLLDESTDIR = $$PWD/bin
    CONFIG += dll
  }
}

DEFINES -= QT_NO_CAST_TO_ASCII QBA_NO_CAST_TO_VOID QBA_NO_CAST_TO_ASCII QT_NO_CAST_FROM_ASCII

